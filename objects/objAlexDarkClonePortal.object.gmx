    image_xscale = scrAlexEaseOutSine(_anim_val, 0, 2.5, anim_dur);
    image_yscale = image_xscale;
    
    if(!collapse &amp;&amp; t &gt;= anim_dur &amp;&amp; t mod 10 == 0) {
        instance_create(x, y, objAlexDarkClonePortalPart);
    }
    
    if(t &gt;= clone_initial_delay + (clone_seperation * clone_amnt)) {
        collapse = true;
        t = anim_dur;
        
