        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Credits timeline

t++


// Intro text
if((t-20) mod (50*7) == 0 &amp;&amp; t &lt;= (50*7)*2) {
    fade_in_text_index++;
    fade_in_text_active = true;
    alarm[1] = 50*5;
}
if(fade_in_text_active) {
    fade_in_text_alpha = min(fade_in_text_alpha + 0.05, 1);
} else {
    fade_in_text_alpha = max(fade_in_text_alpha - 0.015, 0);
}


// Platform rise
if t == 680 {
    with objCreditsPlatform up = true
}

// Credit blocks
if(t &gt;= 930 &amp;&amp; can_drop_block &amp;&amp; !spent) {
    global.creditsindex++;
    can_drop_block = false;

    var p = ds_list_find_value(remaining,0);
    ds_list_delete(remaining,0)
    if ds_list_size(remaining) == 0 {
        spent = true;
        alarm[0] = 250;
        
        // Fade out music
        audio_sound_gain(global.currentMusic, 0, 5000);
        with(objCreditsConfetti) fade_out = true;
    }
    
    d = instance_create(-400,0,objCreditsDude)
    d.idx = p;
    var spr = ds_map_find_value(sprites,p);
    maker = "maker name here"; //maker = ds_map_find_value(names,p);
    d.sprite_index = spr
    d.image_xscale = 64 / sprite_get_width(spr)
    d.image_yscale = 64 / sprite_get_height(spr)
    d.image_speed = 0.2
}

// Cam movement &amp; confetti
if(t &gt; 1100 &amp;&amp; !spent) {
    view_yview[0] -= cam_scroll_spd;
    cam_scrolled_dist += cam_scroll_spd;
    if(cam_scrolled_dist &gt;= cam_dist_per_block) {
        cam_scrolled_dist -= cam_dist_per_block;
        can_drop_block = true;
    }
    
    if(t mod 10 == 0) {
        var _x = random_range(0, 800);
        var _y = view_yview[0] - 100;
        instance_create(_x, _y, objCreditsConfetti);
    }
}

// Credits scrolling
if(t &gt;= 600) {
    if(t == 600) {
        credits_scroll_spd = -0.38;
    }
    
    if(spent) {
        credits_scroll_spd = 0;
    }
    
    if(credits_scroll_spd != 0) {
        credits_y_offset += credits_scroll_spd;
        credits_y = view_yview[0] + credits_y_offset;
    }
}

// Fade out overlay
if(fade_out) {
    fade_out_alpha += 0.005;
    
    if(fade_out_alpha &gt;= 1.3) {
        instance_create(0, 0, objAlexMusicFadeTransition);
        room_goto(rLobbyReception);
    }
}



