draw_sprite(sprAlexStageRatingBar, 0, ui_x-52, ui_y-28);
draw_sprite_part(sprAlexStageRatingBar, 1, 0, 0, 40*ui_rating_length, 8, ui_x-52, ui_y-28);

// Difficulty
scrDrawTextSquareOutline(ui_x+10, ui_y-43, "difficulty", text_col, outline_col, 1, fa_left, fa_top);
draw_sprite(sprAlexStageDifficultyBar, 0, ui_x+13, ui_y-28);
draw_sprite_part(sprAlexStageDifficultyBar, 1, 0, 0, 40*ui_difficulty_length, 8, ui_x+13, ui_y-28);

// Secrets
if(total_secrets &gt; 0) {
    draw_sprite(sprAlexStageSecretIcon, 0, ui_x, ui_y-20);
    scrDrawTextSquareOutline(ui_x, ui_y-13, string(secrets_found)+"/"+string(total_secrets), text_col, outline_col, 1, fa_center, fa_top);
}

draw_set_alpha(1);
