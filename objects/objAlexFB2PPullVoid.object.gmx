        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Logic

image_angle -= 2;

t++;
if(t &gt; 50)
    t -= 50;

glow_scale = scrAlexEaseInSine(t, 2, 0, 50) * image_xscale;
glow_alpha = scrAlexEaseInSine(t, 0, 1, 50) * image_alpha;

// Particles
if(t mod 5 == 0) {
    var _dir = random(360);
    var _part_x = x + lengthdir_x(300, _dir);
    var _part_y = y + lengthdir_y(300, _dir);
    var _part = scrAlexMakeParticleExt(_part_x, _part_y, 50, sprAlexGlowOrb, 0, 0, 0, _dir + 180, 0, 0.2, c_black, c_aqua, bm_normal, true, false, 0, depth-1, false);
        _part.image_alpha = 0;
        _part.add_alpha = 0.01;
}

if(!fade_out) {
    image_alpha = lerp(image_alpha, 1, 0.02);
    image_xscale = lerp(image_xscale, 1, 0.02);
    image_yscale = image_xscale;
    
    pull_strength = min(pull_strength + 0.1, max_pull_strength);
    
    if(pull_strength == max_pull_strength) {
        mask_index = sprite_index;
    }
    
    // Pull player
    if(instance_exists(objAlexFlyingPlayer)) {
        var _len = point_distance(x, y, objAlexFlyingPlayer.x, objAlexFlyingPlayer.y) - pull_strength;
        var _dir = point_direction(x, y, objAlexFlyingPlayer.x, objAlexFlyingPlayer.y);
        objAlexFlyingPlayer.x = x + lengthdir_x(_len, _dir);
        objAlexFlyingPlayer.y = y + lengthdir_y(_len, _dir);
    }
} else {
    if(pull_strength &gt; 0) {
        audio_sound_gain(wormhole_sfx, 0, 1000);
    }

    image_alpha = max(image_alpha - 0.025, 0);
    mask_index = -1;
    pull_strength = 0;
    
    if(image_alpha &lt;= 0) {
        instance_destroy();
    }
}
</string>
