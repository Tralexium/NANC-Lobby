}

if(timer &gt; 700 &amp;&amp; timer &lt; 740) {
    overlay_color = c_white;
    overlay_alpha = min(overlay_alpha + 0.025, 1);
}

if(timer &gt; 748 &amp;&amp; (timer-748) mod 7 == 0 &amp;&amp; letter_index &lt; 5) {
