        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Reveal true form

true_form_revealed = true;

sprite_index = sprAlexFinalBossSmokeAppear
image_index = 0;
image_speed = 0.3;

audio_play_sound(sndAlexFinalStageCutsceneAppear, 0, false);
instance_create(x, y, objAlexScreenPulseEffect);

