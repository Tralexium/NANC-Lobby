            <string>/// Spawn the voids on screen to avoid waiting

if(instance_exists(objPlayer)) {
    if(distance_to_object(objPlayer) &gt; deactivation_range) {
        //instance_destroy();
        exit;
    }
}

var _max_num = 10;
var _curent_num = 0;
var _spawn_x = x;
var _spawn_y = y;
var _len = 0;
var _sprite = noone;

switch(void_type) {
    default:
    case VOID_TYPE.square:
        _sprite = sprAlexDarkVoidSquareCenter;
        break;
    
    case VOID_TYPE.diamond:
        _sprite = sprAlexDarkVoidDiamond;
        break;
        
    case VOID_TYPE.circle:
        _sprite = sprAlexDarkVoidCircle;
        break;
        
    case VOID_TYPE.triangle:
        _sprite = sprAlexDarkVoidTriangle;
        break;
}

while(_curent_num &lt; _max_num) {
    _len = current_dist;
    _spawn_x = x + lengthdir_x(_len, spawn_dir);
    _spawn_y = y + lengthdir_y(_len, spawn_dir);
    
    if(place_meeting(_spawn_x, _spawn_y, objAlexDarkVoidKiller))
        break;
        
    if(_len mod (spawn_period*spawn_spd) == 0) {
        var _inst = instance_create(_spawn_x, _spawn_y, objAlexMovingDarkVoid);
            _inst.sprite_index = _sprite;
            _inst.direction = spawn_dir;
            _inst.speed = spawn_spd;
            _inst.image_xscale = spawn_scale;
            _inst.image_yscale = spawn_scale;
            _inst.rot_spd = spawn_rot_spd;
            
        _curent_num++
    }
    
    current_dist++;
}
