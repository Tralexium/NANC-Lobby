        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Logic

t++;

if(vanish) {
    speed = max(speed-0.1, 0);
}
else if(active) {
    var _dir = 0;
    var _dist = 0;

    if(instance_exists(objPlayer)) {
        _dir = point_direction(x, y, objPlayer.x, objPlayer.y);
        _dist = point_distance(x, y, objPlayer.x, objPlayer.y);
        
        if(_dist &lt; kill_range) {
            scrKillPlayer();
        }
    } else {
        event_user(0);
    }
    
    var _target_spd = min_spd;
    if(_dist &gt; min_dist)
        _target_spd = clamp(_dist / dist_per_decimal_spd, min_spd, max_spd);
        
    direction = _dir;
    speed = lerp(speed, _target_spd, 0.05);
}
</string>
          </argument>
        </arguments>
      </action>
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Animations

// Body rotation
body_angle += body_angle_add;


// Facial movement
var _target_x = x;
var _target_y = y+1;

if(instance_exists(objPlayer) &amp;&amp; active &amp;&amp; !vanish) {
    _target_x = objPlayer.x;
    _target_y = objPlayer.y;
    
    mouth_move_len = lerp(mouth_move_len, mouth_move_len_target, 0.2);
    eye_move_len = lerp(eye_move_len, eye_move_len_target, 0.2);
    pupil_move_len = lerp(pupil_move_len, pupil_move_len_target, 0.2);
} else {
    mouth_move_len = lerp(mouth_move_len, 0, 0.2);
    eye_move_len = lerp(eye_move_len, 0, 0.2);
    pupil_move_len = lerp(pupil_move_len, 0, 0.2);
}

if(vanish) {
    if(round(mouth_move_len) == 0) {
        mouth_open_amnt = max(mouth_open_amnt-0.1, 0);
        mouth_idle_movement_amnt = max(mouth_idle_movement_amnt-0.025, 0);
        eye_open_amnt = max(eye_open_amnt-0.1, 0);
    }
    
    if(round(mouth_open_amnt) == 0) {
        shrink_t++;
        
        body_scale = scrAlexEaseInSine(shrink_t, 1, 0, shrink_dur)
        
        if(shrink_t &gt;= shrink_dur) {
            if(audio_is_playing(sndAlexChaseMonsterAmbient))
                audio_stop_sound(sndAlexChaseMonsterAmbient);
        
            instance_destroy();
        }
    }
}

var _dir = point_direction(x, y, _target_x, _target_y);

var _target_mouth_x = x + mouth_pivot_x + lengthdir_x(mouth_move_len, _dir);
var _target_mouth_y = y + mouth_pivot_y + lengthdir_y(mouth_move_len, _dir);
mouth_x = lerp(mouth_x, _target_mouth_x, mouth_move_lerp);
mouth_y = lerp(mouth_y, _target_mouth_y, mouth_move_lerp);

var _target_eye_x = x + eye_pivot_x + lengthdir_x(eye_move_len, _dir);
var _target_eye_y = y + eye_pivot_y + lengthdir_y(eye_move_len, _dir);
eye_x = lerp(eye_x, _target_eye_x, eye_move_lerp);
eye_y = lerp(eye_y, _target_eye_y, eye_move_lerp);

var _target_pupil_x = lengthdir_x(pupil_move_len, _dir);
var _target_pupil_y = lengthdir_y(pupil_move_len, _dir);
pupil_x = lerp(pupil_x, _target_pupil_x, pupil_move_lerp);
pupil_y = lerp(pupil_y, _target_pupil_y, pupil_move_lerp);


// Facial scales
if(!eye_has_blinked) {
    eye_blink_timer++;
    var _blink_spd = 180/eye_blink_dur;
    
    eye_blink_amnt = abs(dcos(eye_blink_timer * _blink_spd));
    
    if(eye_blink_timer &gt;= eye_blink_dur)
        eye_has_blinked = true;
} else if(!alarm[0] &amp;&amp; active) {
    eye_blink_amnt = 1;
    alarm[0] = eye_blink_frequency + irandom(50);
}

var _mouth_scale = mouth_open_amnt + (dsin(t*mouth_idle_movement_spd)*mouth_idle_movement_amnt);
var _eye_scale = eye_open_amnt * eye_blink_amnt;
var _pupil_scale = pupil_dilation * eye_blink_amnt;

mouth_x_scale = _mouth_scale - (mouth_squish_mult*(_mouth_scale-1));
mouth_y_scale = _mouth_scale;

eye_x_scale = _eye_scale - (eye_squish_mult*(_eye_scale-1));
eye_y_scale = _eye_scale;

pupil_x_scale = _pupil_scale;
pupil_y_scale = _pupil_scale;
</string>
          </argument>
