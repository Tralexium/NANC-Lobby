        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Intro Cutscene

if(!cutscene_finished) {
    if(t == 1) {
        mouth_idle_movement_amnt = 0;
    }

    if(t &gt;= 50 &amp;&amp; t &lt;= 250) {
        if(t == 50)
            audio_play_sound(sndAlexChaseMonsterCutsceneGlow, 0, false);    
        
        if(t mod 10 == 0)
            part_particles_create(part_system, x, y, part_type_flare, 1);
            
        if(t mod 15 == 0)
            part_particles_create(part_system, x, y, part_type_beam, 1);
    }
    
    if(t &gt; 100 &amp;&amp; t &lt;= 140) {
        if(t == 101) {
            audio_sound_pitch(sndAlexChaseMonsterGrowSmall, 0.8);
            audio_play_sound(sndAlexChaseMonsterGrowSmall, 0, false);
        }
            
        var _anim_val = clamp(t-100, 0, 40);
        body_scale = scrAlexEaseOutElastic(_anim_val, 0, 0.1, 40);
    }
    
    if(t &gt; 170 &amp;&amp; t &lt;= 200) {
        if(t == 171) {
            audio_sound_pitch(sndAlexChaseMonsterGrowSmall, 1);
            audio_play_sound(sndAlexChaseMonsterGrowSmall, 0, false);
        }
    
        var _anim_val = clamp(t-170, 0, 30);
        body_scale = scrAlexEaseOutElastic(_anim_val, 0.1, 0.25, 30);
    }
    
    if(t &gt; 220 &amp;&amp; t &lt;= 250) {
        if(t == 221) {
            audio_sound_pitch(sndAlexChaseMonsterGrowSmall, 0.9);
            audio_play_sound(sndAlexChaseMonsterGrowSmall, 0, false);
        }
    
        var _anim_val = clamp(t-220, 0, 30);
        body_scale = scrAlexEaseOutElastic(_anim_val, 0.25, 0.5, 30);
    }
    
    if(t &gt; 270 &amp;&amp; t &lt;= 310) {
        if(t == 271) {
            audio_play_sound(sndAlexChaseMonsterGrowBig, 0, false);
        }
    
        var _anim_val = clamp(t-270, 0, 40);
        body_scale = scrAlexEaseOutElastic(_anim_val, 0.5, 1, 40);
    }
    
    if(t &gt; 340 &amp;&amp; t &lt;= 360) {
        if(t == 341) {
            audio_play_sound(sndAlexChaseMonsterFaceReveal, 0, false);
        }
    
        var _anim_val = clamp(t-340, 0, 20);
        eye_open_amnt = scrAlexEaseOutSine(_anim_val, 0, 1, 20);
    }
    
    if(t &gt; 390 &amp;&amp; t &lt;= 420) {
        var _anim_val = clamp(t-390, 0, 30);
        mouth_open_amnt = scrAlexEaseOutSine(_anim_val, 0, 1, 30);
        
        mouth_idle_movement_amnt = 0.15;
    }
    
    if(t &gt; 450 &amp;&amp; t &lt;= 460) {
        if(t == 451) {
            audio_play_sound(sndAlexChaseMonsterRoar, 0, false);
        }
    
        var _anim_val = clamp(t-450, 0, 10);
        mouth_open_amnt = scrAlexEaseOutSine(_anim_val, 1, 1.5, 10);
        eye_y = scrAlexEaseOutSine(_anim_val, y - 50, y - 80, 10);
    }
    
    if(t == 460) {
        mouth_idle_movement_spd = 50;
        mouth_idle_movement_amnt = 0.3;
    }
    
    if(t &gt; 460 &amp;&amp; t &lt; 520) {
        eye_y = y - 80;
        
        if(t mod 5 == 0)
            instance_create(x, y, objAlexScreenPulseEffect2);
    }
    
    if(t &gt; 520 &amp;&amp; t &lt;= 540) {
        mouth_idle_movement_spd = 1.75;
        mouth_idle_movement_amnt = 0.15;
        
        var _anim_val = clamp(t-520, 0, 20);
        mouth_open_amnt = scrAlexEaseOutSine(_anim_val, 1.5, 1, 20);
        eye_y = scrAlexEaseOutSine(_anim_val, y - 80, y - 50, 20);
    }
    
    if(t == 580) {
        cutscene_finished = true;
        active = true;
    }
} else {
    // Quick spawn
    
    if(t == 1) {
        body_scale = 1;
        mouth_open_amnt = 1;
        eye_open_amnt = 1;
        
        active = true;
        
        scrAlexShakeCam(20, 10, 10, 3, 4);
        audio_play_sound(sndAlexChaseMonsterQuickSpawn, 0, false);
    }
    
    if(t &gt; 0 &amp;&amp; t &lt;= 50) {
        spawn_glow_alpha = scrAlexEaseOutSine(t, 1, 0, 50);
    }
}
</string>
