    scrFBPShockwave(x, y, 400 - round(200 * target_scale));
    
    var _new_bounce_speed = abs(vspeed) * bounce_multi
    var _new_target_scale = target_scale - decrease_per_split;
    if(_new_target_scale &gt; 0) {
        scrFBPBubble(x, y, target_scale + new_start_scale_add, _new_target_scale, 105, _new_bounce_speed);
        scrFBPBubble(x, y, target_scale + new_start_scale_add, _new_target_scale, 75, _new_bounce_speed);
    }
    
    instance_destroy();
}

start_scale = lerp(start_scale, target_scale, 0.15);
image_xscale = start_scale;
image_yscale = start_scale;
